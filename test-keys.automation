from selenium import webdriver
options = webdriver.ChromeOptions()
options.add_experimental_option("excludeSwitches", ["enable-logging"])
driver=webdriver.Chrome(options=options, executable_path=r'C:/Users/…/…/chromedriver.exe')
driver.get(https://b2c.passport.rt.ru)
# Поиск кнопки «Телефон» и клик по ней
phone=driver.find_element_by_xpath(//div[@id="t-btn-tab-phone"])
phone.click()
#  Поиск элементов и присваивание к переменным
Input_mobile_phone = driver.find_element_by_xpath(//input[@id="username"])
Input_password = driver.find_element_by_xpath(//input[@name="password"])
login_button = driver.find_element_by_xpath(//button[@id="kc-login"])
# Действия с формами - заполнение формы и нажатие кнопки
Input_mobile_phone.send.keys(“standard_user”)
Input_password.send.keys(“secret_password”)
login_button.send.keys(“Keys.RETURN”)
# Поиск и проверка попадания на необходимую страницу
title_text = driver.find_element_by_xpath(//input[text()=”Личный кабинет”])
if title_text.text == "Личный кабинет":
    print("Тест выполнен успешно")
else:
    print("Ошибка")






